{
  "name": "core",
  "version": "1.0.5",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**",
    "./LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/konrad-marzec/heat-calendar.git"
  },
  "description": "heat-calendar is a JavaScript library designed for producing time-series calendar heatmap on top of SVG.",
  "keywords": [
    "calendar",
    "react",
    "react-component",
    "svg",
    "heatmap",
    "graph",
    "data visualization"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc -p ./tsconfig.json",
    "coverage": "jest --coverage",
    "build": "pnpm build:js && pnpm build:ts",
    "build:ts": "swc src --out-dir dist --config-file ./.swcrc",
    "build:js": "swc src --out-dir dist --config-file ./.swcrc",
    "dev": "swc src --watch --out-dir dist --config-file ./.swcrc",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@types/testing-library__jest-dom": "^5.14.6",
    "eslint": "^8.42.0",
    "eslint-config-base": "workspace:*",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
