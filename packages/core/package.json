{
  "name": "core",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "swc src --out-dir dist --config-file ./.swcrc",
    "dev": "swc src --watch --out-dir dist --config-file ./.swcrc",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "eslint": "^8.42.0",
    "eslint-config-base": "workspace:*",
    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
